<dds>
    <!-- Qos Library -->
    <qos_library name="MyWriterLibrary">         
		<publisher_qos name = "PartitionA_Publisher"> <!-- This is a shortcut for defining a QoS profile with a single QoS. See User's Manual 15.7.3. -->
            <partition>
                 <name>
					<element>A</element> <!-- This is how you list a sequence of elements. See User's Manual 15.6.2. -->
				</name>
			</partition>                
        </publisher_qos>
        <publisher_qos name = "PartitionB_Publisher">
            <partition>
                 <name>
					<element>B</element>
				</name>
			</partition>                
        </publisher_qos>						
        <writer_qos name = "ExclusiveWriter">
			<ownership>
				<kind>DDS_EXCLUSIVE_OWNERSHIP_QOS</kind>
			</ownership>
		</writer_qos>
	</qos_library>
    <!-- End of Qos Library -->
</dds>